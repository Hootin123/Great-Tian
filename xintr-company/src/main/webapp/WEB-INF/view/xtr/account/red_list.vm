<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01//EN"
        "http://www.w3.org/TR/html4/strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>我的账户 - 红包</title>
    #parse("comm/head.vm")
    <link href="#springUrl('/resource/css/account/red_list.css')" rel="stylesheet">
</head>
<body>

<div>
    <h1 class="top">我的红包</h1>
##    <div class="close"><img src="#springUrl('/resource/img/ture.png')" alt=""></div>
    <div class="con">
        <ul>
            #foreach($data in $!{redData})
            <li>
                #if($!{data.sourceType} == 1)
                <a class="red" href="#">注册红包</a>
                #end
                <span red_id="$!{data.redId}" class="details red_detail">详情描述</span>
                <span class="eight">$!{data.redMoney}元</span>
                <span class="be">有效期：</span>
                <span class="time">$!date.format('yyyy-MM-dd',$!{data.useStartTime})-$!date.format('yyyy-MM-dd',$!{data.useEndTime})</span>
            </li>
            #end
        </ul>
    </div>
    <div class="footer1">
        <a href="javascript:void(0);" onclick="history.back();">确认</a>
    </div>

    <div class="pacity"></div>
    <div style="" class="aboxbg" id="aboxbg">
        <img  class="abImg" src="#springUrl('/resource/img/shut.png')" alt="">
        <div class="abox" id="abox">
            <div class="abox_head">
                <h3>红包详情</h3>
            </div>
            <span class="span1 red_title"></span>
            <span class="span2 red_money"></span>
            <span class="span3">详情描述：</span>
            <div class="red_desc"></div>
            <i class="btn"><span>确认</span></i>
        </div>
    </div>
</div>

<script type="text/javascript" src="#springUrl('/resource/js/account/red_list.js')"></script>
</body>