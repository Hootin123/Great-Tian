    .xtrHint-main table tr td{  border:1px solid #dbe9ee;}
    .xtrHint-footer a{ display: block;  width:200px;  height: 40px;  margin: 0 auto;  margin-top: 30px;  color: #fff;  background:#0696fa;  border-radius: 6px;  font-size: 14px;  line-height: 40px;  text-align: center;  font-family: '微软雅黑';  }
    .xtrHint-text{  width: 100%;  height: 30px;  font-size: 14px;  color: #f9a225;  text-align: center;  line-height: 70px;}
    .allTr, .allTr+tr{background: #feebe7}
    tbody .activeTr{background: #d2f8fb}
