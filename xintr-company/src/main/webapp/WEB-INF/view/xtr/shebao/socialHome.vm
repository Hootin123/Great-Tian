    .batchArea,.batchFundArea{line-height: 30px;width:80%;margin:23px auto 0;height:30px;background:url(/resource/img/salary/san.png) no-repeat  295px ;}
    .batchArea1,.batchFundArea1{width:30%;float:left;}
    .batchArea2,.batchFundArea2{width:67%;float:left;padding-left:2%;border:1px solid #ccc;}
    .batchArea3,.batchFundArea3{width:68%;display:block;height:30px;text-indent:2%;font- family:"微软雅黑";}
    .batchSocialLi,.batchFundArealli{width:55.2%;border:1px solid #ccc;margin- left:34%;margin-top:1px;text-indent:2%;line-height: 30px}
    .batchSocialLi li:hover,.batchFundArealli li:hover{background:#0696fa;color:#fff;max- height:150px;overflow: auto;}
    .makeSocial,.makeFund{width:500px;background:#fff;z-index:20;border- radius:20px;position: absolute;top:35%;left:35%;display: none}
    .makeSocialCon,.makeFundCon{width:80%;margin:0 auto;overflow: hidden}
    .makeSocialMend,.makeFundMend{margin-top:25px;line-height: 30px}
