    .aboxbgBtn2,.sociaBtn2,.batchSocialBtn2,.batchFundBtn2,.makeSocialBtn2,.makeFundBtn2 {width:200px;height:35px;background:#fff;color:#0696fa;border:1px solid #0696fa;float:right;font-family:"微软雅黑";border-radius: 6px;cursor: pointer }
    .sociaAdjust,.fundAdjust{width:500px;height:300px;background:#fff;z-index:20;position: absolute;top:0;bottom:0;left:0;right:0;margin:auto;border-radius: 20px;display: none }
