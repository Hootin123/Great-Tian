    .sociaAdjust1{line-height:30px;margin-top:20px;padding-left:70px;color:#666;}
    .sociaAdjust2{line-height:30px;margin-top:20px;padding-left:70px;color:#666;}
    .sociaAdjust2 input{height:25px;width:130px;padding-left:20px;color:#666;}
    .sociaAdjust2 span{padding-left:20px;color:#f99b15;}
    .sociaAdjust3{line-height:30px;margin-top:20px;padding-left:70px;color:#666;}
    .batchSocial,.batchFund{width:500px;background:#fff;z-index:20;border- radius:20px;position: absolute;top:35%;left:35%;display:none}
    .batchSocial h3,.batchFund h3,.makeSocial h3,.makeFund h3,.mayArea h3{margin-top:30px;text-align: center}
    .batchSocialCon,.batchFundCon{width:80%;margin:0 auto;line-height:30px;margin- top:20px;color:#666;}
    /*.batchSocialCon1{border:1px solid #dbe9ee;width:230px;height:25px;padding-
left:20px;font-family: "微软雅黑";margin-left:20px;background:url
(/resource/img/salary/san.png) no-repeat  230px ;}*/
