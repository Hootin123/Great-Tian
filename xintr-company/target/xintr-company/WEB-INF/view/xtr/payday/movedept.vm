    <div class="moveTo_left" style="margin-left:25px">
        <h4>选择成员所在部门</h4>
        <dl style="height: 300px;overflow: auto;">

            #if($!{companyTree} && $!{companyTree.size()} > 0)

                <dt><em></em>$!companyTree.get(0).companyName</dt>
                #foreach($data in $!{companyTree})
                    <dd>
                        <em></em>
                        <a href="#" onclick="onSelectDept($!{data.depId},'$!{data.depName}')">$!{data.depName}</a>
                    </dd>
                #end
            #end

        </dl>
    </div>
    <div class="moveTo_right">
        <h4>成员将属于以下部门</h4>

        <div>
            <a id="deptName"></a>
            <input type="hidden" id="deptId" name="deptId" value="">
        </div>
    </div>
    <div style="clear: both;"></div>
    <div class="move_btn">
        <a id="okBtn" href="javascript:;">确定</a>
        <a id="cancelBtn" href="javascript:;">取消</a>
    </div>

</div>
