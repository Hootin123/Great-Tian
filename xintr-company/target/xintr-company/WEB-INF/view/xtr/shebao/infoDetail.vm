<style type="text/css">
	.inforDetalisHeader{
		width: 100%;
	    min-width: 1000px;
	    height: 80px;
	    font-size: 24px;
	    font-family: '微软雅黑';
	    text-align: center;
	    line-height: 80px;
	    border-bottom: 1px solid #dbe9ee;
	    position: relative;
	}
	.inforDetalisHeader img{
		position: absolute;
	    top: 0;
	    bottom: 0;
	    right: 30px;
	    margin: auto;
	    cursor: pointer;
	}
	.inforDetalisMain{
		margin: 0 auto;
