<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xtr.core.persistence.writer.customer.CustomerWechatWriterMapper" >
<insert id="saveCustomerBind" parameterType="com.xtr.api.domain.customer.CustomerWechatBean" useGeneratedKeys="true">
