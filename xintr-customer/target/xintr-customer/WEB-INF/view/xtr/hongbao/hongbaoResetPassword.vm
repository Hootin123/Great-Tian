########红包设置密码页面

<!DOCTYPE html>
<html lang="en" id="html">
<head>
    <meta charset="UTF-8">
    <title>忘记密码</title>
    #parse("comm/head.vm")
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=no,minimum-scale=1,maximum-scale=1">
    <script type="text/javascript" src=#springUrl('/resource/js/hongbao/forget.js')></script>
    <link rel="stylesheet" type="text/css" href=#springUrl('/resource/css/hongbao/forget.css')>
</head>
<body>
<header>
    忘记密码
</header>
<section>

    <form id="resetForm">
    <ul>
        <li id='phone'>
            <label>手机号码</label><input type="text" id="memberPhone" name="phoneNum" placeholder='手机号码'>
            <b></b>
            <p>
                <em></em>
                <span>获取验证码</span>
            </p>

        </li>
        <li id="phonecode">
            <label>短信验证码</label><input type="text" name="phoneCode" placeholder='验证码'>
            <b></b>
            <em></em>
        </li>
        <li id='password'>
            <label>新密码</label><input type="password" name="newPassword" placeholder ='登录密码'>
            <b></b>
            <em></em>
        </li>
        <li id='repwd'>
            <label>确认密码</label><input type="password" name="confirmPassword" placeholder ='确认密码'>
            <b></b>
            <em></em>
        </li>
    </ul>
    </form>
    <div class="hint" style="display: none">验证码获取错误</div>
    <div class="affirm">确认</div>
    <div class="returnLogin">
        <i></i>
        返回登录
    </div>
</section>
</body>
</html>
