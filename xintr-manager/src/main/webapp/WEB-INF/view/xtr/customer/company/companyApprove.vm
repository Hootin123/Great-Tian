<!DOCTYPE html>
<html>

<head>
    <title>H+ 企业资料审核</title>
    #parse("comm/head.vm")
    <link href="#springUrl('/resource/css/hui/plugins/bootstrap-table/bootstrap-table.min.css')" rel="stylesheet">
    <link href="#springUrl('/resource/css/payfor1.css')" rel="stylesheet" />
</head>

<body>
<div class="col-sm-12">
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="panel panel-default" style="margin-top:10px;width:70%;margin-left:15%;">
                <input type="hidden" value="$!{companyApproveDetail.companyId}" id="companyApproveId">
                <table id="companyApproveTab" data-mobile-responsive="true" class="table table-hover table-bordered">
                    <tr><td colspan="2" class="text-center"><strong>基本信息</strong></td></tr>
                    <tr><td style="width:20%;">公司名称</td><td style="width:50%;">$!{companyApproveDetail.companyName}</td></tr>
                    <tr>
                        <td>公司logo</td>
                        <td>
                            #if($companyApproveDetail.companyLogo)
                                <img src="browseFile.htm?fileName=$!{companyApproveDetail.companyLogo}" widht="300" height="250"/>
                            #end
                        </td>
                    </tr>
                    <tr><td>公司地址</td><td>$!{companyApproveDetail.companyAddress}</td></tr>
                    <tr><td>营业执照编号</td><td>$!{companyApproveDetail.companyNumber}</td></tr>
                    <tr>
                        <td>营业执照扫描件</td>
                        <td>
                            #if($companyApproveDetail.companyOrganizationImg)
                                <img src="browseFile.htm?fileName=$!{companyApproveDetail.companyOrganizationImg}" widht="400" height="300"/>
                            #end
                        </td>
                    </tr>
                    <tr><td>法人代表</td><td>$!{companyApproveDetail.companyCorporation}</td></tr>
                    <tr><td>联系电话</td><td>$!{companyApproveDetail.companyContactTel}</td></tr>
                    <tr><td>公司规模</td><td>$!{companyApproveDetail.companyScale}</td></tr>
                    <tr><td>所属行业</td><td>$!{companyApproveDetail.companyBelongIndustry}</td></tr>
                    <tr>
                        <td>了解薪太软的渠道</td>
                        <td>
                            #if($companyApproveDetail.companyChannel==1)
                                百度、360等搜索引擎
                            #elseif($companyApproveDetail.companyChannel==2)
                                微信、QQ等社交平吧
                            #elseif($companyApproveDetail.companyChannel==3)
                                推销
                            #elseif($companyApproveDetail.companyChannel==4)
                                广告
                            #elseif($companyApproveDetail.companyChannel==5)
                                朋友介绍
                            #else
                                其他
                            #end
                        </td>
                    </tr>
                    <tr style="height: 100px;">
                        <td style="border:2px #000000 solid;">驳回原因</td>
                        <td style="border:2px #000000 solid;">
                            <textarea style="width: 100%;height: 100%;margin:0 0 0 0;" name="companyAuditRemark">$!{companyApproveDetail.companyAuditRemark}</textarea>
                        </td>
                    </tr>
                </table>
            </div>
            <div class="text-center" style="margin-top:200px;margin-bottom:20px;">
                <button type="button" id="approveBtn" class="btn btn-w-m btn-success" >审核通过</button>
                <button type="button" id="noApproveBtn" class="btn btn-w-m btn-white" >驳回</button>
            </div>
        </div>
    </div>

</div>

</body>

<script src="#springUrl('/resource/js/hui/content.min.js')"></script>
<script src="#springUrl('/resource/js/hui/plugins/bootstrap-table/bootstrap-table.min.js')"></script>
<script src="#springUrl('/resource/js/hui/plugins/bootstrap-table/bootstrap-table-mobile.min.js')"></script>
<script src="#springUrl('/resource/js/hui/plugins/bootstrap-table/locale/bootstrap-table-zh-CN.min.js')"></script>
<script src="#springUrl('/resource/js/hui/plugins/validate/jquery.validate.min.js')"></script>
<script src="#springUrl('/resource/js/comm/jdatagrid.js')"></script>
<script src="#springUrl('/resource/js/customer/company/companyApprove.js')"></script>
</html>