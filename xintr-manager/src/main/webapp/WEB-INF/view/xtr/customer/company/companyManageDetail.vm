<!DOCTYPE html>
<html>

<head>
    <title>H+ 员工信息管理</title>
    #parse("comm/head.vm")
    <link href="#springUrl('/resource/css/hui/plugins/bootstrap-table/bootstrap-table.min.css')" rel="stylesheet">
    <link href="#springUrl('/resource/css/payfor1.css')" rel="stylesheet" />
</head>

<body>
<input type="hidden" name="flag" value="$!{flag}"/>
<div class="col-sm-12">
    <div class="container-fluid">
        <div class="row-fluid">
            #if($isReturnParam==1)
                <div class="text-right" style="margin-top: 10px;">
                    <button class="btn btn-primary" type="button" id="backBtn" onclick="history.go(-1)"><i class="fa fa-search"></i>&nbsp;&nbsp;&nbsp;返回
                    </button>
                </div>
            #end

            <div class="panel panel-default" style="margin-top:10px;">
                <input type="hidden" value="$!{companyDetail.companyId}" id="companyIdHid">
                <input type="hidden" value="$!{isReturnParam}" id="isReturnParamParam">
                <table id="companyManageTab" data-mobile-responsive="true" class="table table-hover table-bordered">
                    <tr><td colspan="2" class="text-center"><strong>基础信息</strong></td><td colspan="2" class="text-center"><strong>账户信息</strong></td></tr>
                    <tr><td style="width:15%;">公司名称（企业编号）</td><td style="width:30%;">$!{companyDetail.companyName}（$!{companyDetail.companyId}）</td><td style="width:15%;">超级管理员用户名</td><td style="width:30%;">$!{companyDetail.memberLogname}</td></tr>
                    <tr>
                        <td>公司logo</td>
                        <td>
                            #if($companyDetail.companyLogo && $companyDetail.companyLogo != "")
                                <a href="browseFile.htm?fileName=$!{companyDetail.companyLogo}">下载</a>
                            #end
                        </td>
                        <td>账户余额</td>
                        <td>$!{companyDetail.totalAmount}</td></tr>
                    <tr><td>公司地址</td><td>$!{companyDetail.companyAddress}</td><td>注册时间</td><td>$!date.format('yyyy-MM-dd HH:mm:ss',$!{companyDetail.companyAddime})</td></tr>
                    <tr><td>公司规模</td><td>$!{companyDetail.companyScale}</td><td>最近登录时间</td><td>$!date.format('yyyy-MM-dd HH:mm:ss',$!{companyDetail.memberLoginRecentTime})</td></tr>
                    <tr>
                        <td>营业执照编号</td>
                        <td>$!{companyDetail.companyNumber}</td>
                        <td>签约状态</td>
                        <td>$!{companyDetail.protocolState}</td>
                    </tr>
                    <tr>
                        <td>营业执照扫描件</td>
                        <td>
                            #if($companyDetail.companyOrganizationImg)
                                <a href="browseFile.htm?fileName=$!{companyDetail.companyOrganizationImg}">下载</a>
                            #end
                        </td>
                        <td>实名认证状态</td>
                        <td>
                            #if ($companyDetail.companyIsauth==1)
                                已认证
                            #elseif($companyDetail.companyIsauth==2)
                                #if ($companyDetail.companyCertificationAmount and $companyDetail.companyCertificationAmount!=0)
                                    待认证($companyDetail.companyCertificationAmount)
                                #else
                                    待认证
                                #end
                            #elseif($companyDetail.companyIsauth==3)
                                认证失败
                            #else
                                未认证
                            #end

                            $!companyDetail.companyPayNumber
                            #if($!companyDetail.companyPayStatus == 0)
                                <span id="paystatus">等待中</span> <a href="javascript:queryPayStatus('$!companyDetail.companyPayNumber', '$!companyDetail.companyId')">查询</a>
                            #elseif($!companyDetail.companyPayStatus == 1)
                                成功
                            #elseif($!companyDetail.companyPayStatus == 2)
                                失败，$!companyDetail.companyPayReason
                            #elseif($!companyDetail.companyPayStatus == 3)
                                退款，$!companyDetail.companyPayReason
                            #end
                        </td>
                    </tr>
                    <tr>
                        <td>法人代表</td><td>$!{companyDetail.companyCorporation}</td>
                        <td colspan="2" style="background: #8c8c8c;text-align: center;">提现银行</td>
                    </tr>
                    <tr><td>法人身份证号码</td><td>$!{companyDetail.companyCorporationIdcard}</td><td>提现银行</td><td>$!{companyDepositBean.depositBankName}</td></tr>
                    <tr><td>所属行业</td><td>$!{companyDetail.companyBelongIndustry}</td><td>提现账户名</td><td>$!{companyDepositBean.depositBankAccountName}</td></tr>
                    <tr>
                        <td>了解薪太软的渠道</td>
                        <td>
                            #if ($companyDetail.companyChannel==1)
                                百度、360等搜索引擎
                            #elseif($companyDetail.companyChannel==2)
                                微信、QQ等社交平吧
                            #elseif($companyDetail.companyChannel==3)
                                推销
                            #elseif($companyDetail.companyChannel==4)
                                广告
                            #elseif($companyDetail.companyChannel==5)
                                朋友介绍
                            #elseif($companyDetail.companyChannel==6)
                                其他
                            #end
                        </td>
                        <td>提现账号</td>
                        <td>$!{companyDepositBean.depositBankNumber}</td>
                    </tr>
                    <tr>
                        <td>资料审核状态</td>
                        <td>
                            #if ($companyDetail.companyAuditStatus==1)
                            审核驳回
                            #elseif($companyDetail.companyAuditStatus==2)
                            审核通过
                            #else
                            待审核
                            #end
                        </td>
                        <td>开户支行名称</td>
                        <td>$!{companyDepositBean.depositSubBankName}</td>
                    </tr>
                </table>
            </div>

            #*<a href='#' onclick='onModifyCompanyProtocol($!{companyDetail.companyId})' >修改</a>*#
            <div class="tabs-container">
                <ul class="nav nav-tabs" id="companyManageDetailUl">
                    <li class="active" id="recharge"><a data-toggle="tab" href="#tab-1" aria-expanded="true">充值记录</a>
                    </li>
                    <li class="" id="deposit"><a data-toggle="tab" href="#tab-2" aria-expanded="false">提现记录</a>
                    </li>
                    <li class="" id="payoff"><a data-toggle="tab" href="#tab-3" aria-expanded="false">发工资记录</a>
                    </li>
                    <li class="" id="protocol"><a data-toggle="tab" href="#tab-4" aria-expanded="false">签约记录</a>
                    </li>
                    <li class="" id="company_note"><a data-toggle="tab" href="#tab-5" aria-expanded="false">联系小记</a>
                    </li>
                    <li class="" id="fileResource"><a data-toggle="tab" href="#tab-6" aria-expanded="false">征信资料库</a>
                    </li>
                </ul>
                <div class="tab-content" id="companyManageDetailLabel">
                    #if($isReturnParam==3)
                        <div id="tab-1" class="tab-pane">
                            <div class="panel-body">
                                <table id="data-list1"></table>
                            </div>
                        </div>
                    #else
                        <div id="tab-1" class="tab-pane active">
                            <div class="panel-body">
                                <table id="data-list1"></table>
                            </div>
                        </div>
                    #end
                    <div id="tab-1" class="tab-pane active">
                        <div class="panel-body">
                            <table id="data-list1"></table>
                        </div>
                    </div>
                    <div id="tab-2" class="tab-pane">
                        <div class="panel-body">
                            <table id="data-list2"></table>
                        </div>
                    </div>
                    <div id="tab-3" class="tab-pane">
                        <div class="panel-body">
                            <table id="data-list3"></table>
                        </div>
                    </div>
                    <div id="tab-4" class="tab-pane">
                        <div class="panel-body">
                            <table id="data-list4"></table>
                        </div>
                    </div>
                    #if($isReturnParam==3)
                        <div id="tab-5" class="tab-pane active">
                            <div class="panel-body">
                                <table id="data-list5"></table>
                            </div>
                        </div>
                    #else
                        <div id="tab-5" class="tab-pane">
                            <div class="panel-body">
                                <table id="data-list5"></table>
                            </div>
                        </div>
                    #end
                    <div id="tab-6" class="tab-pane">
                        <div class="panel-body">
                            #*<table id="data-list6"></table>*#
                            <div class="identity-content-boxs" style="margin-top:20px;margin-bottom:10px;">
                                <div class="identity-content-box">
                                #*<a href="companyCreditSearch.htm?companyId=$!{companyDetail.companyId}" id="downloadFile">下载文件</a>*#
                                    <a  id="downloadFile">下载文件</a>
                                </div>
                                <p>征信资料</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>

</body>

<script src="#springUrl('/resource/js/hui/content.min.js')"></script>
<script src="#springUrl('/resource/js/hui/plugins/bootstrap-table/bootstrap-table.min.js')"></script>
<script src="#springUrl('/resource/js/hui/plugins/bootstrap-table/bootstrap-table-mobile.min.js')"></script>
<script src="#springUrl('/resource/js/hui/plugins/bootstrap-table/locale/bootstrap-table-zh-CN.min.js')"></script>
<script src="#springUrl('/resource/js/hui/plugins/validate/jquery.validate.min.js')"></script>
<script src="#springUrl('/resource/js/comm/jdatagrid.js')"></script>
<script src="#springUrl('/resource/js/customer/company/companyManageDetail.js')"></script>
</html>