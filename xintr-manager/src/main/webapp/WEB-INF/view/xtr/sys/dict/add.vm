<!DOCTYPE html>
<html>

<head>
    <title>新增字典</title>
    #parse("comm/head.vm")
</head>


<div class="ibox" style="padding-top: 0px">


    <div class="ibox-content">
        <form class="form-inline m-t" id="editForm">
            #if($dict)##修改数据
                <input type="text" id="id" name="id" class="hidden" value="$dict.id"/>
            #end
            <input type="text" id="dictValue" name="dictValue" class="hidden" value="$dictValue"/>

            <div class="form-group">
                <label class="col-sm-3 control-label">关键字：</label>

                <div class="col-sm-8">
                    <input id="dictdataValue" name="dictdataValue" class="form-control" type="text"
                           ondragenter="return false" onkeyup="this.value=replaceChinese(this.value)"
                           #if($dict)value="$dict.dictdataValue" #end>
                </div>
            </div>

            <div class="form-group">
                <label class="col-sm-3 control-label">值：</label>

                <div class="col-sm-8">
                    <input id="dictdataName" name="dictdataName" class="form-control" type="text"
                           #if($dict)value="$dict.dictdataName" #end>
                </div>
            </div>

            <div class="form-group">
                <label class="col-sm-3 control-label">排序：</label>

                <div class="col-sm-8">
                    <input id="sort" name="sort" class="form-control" type="number"
                           #if($dict)value="$dict.sort" #else value="0"#end>
                </div>
            </div>


            <div class="form-group">
                <label class="col-sm-2 control-label">值是否固定：</label>

                <div class="col-sm-8">
                    <select class="form-control m-b" id="isfixed" name="isfixed">
                        <option value="0" #if(($dict && $dict.isfixed == 0) || !$role)selected="selected"#end>不固定
                        </option>
                        <option value="1" #if($dict && $dict.isfixed == 1)selected="selected"#end>固定</option>
                    </select>
                </div>
            </div>

        </form>
    </div>
</div>
<script src="#springUrl('/resource/js/sys/dict/addDict.js')"></script>
</html>
