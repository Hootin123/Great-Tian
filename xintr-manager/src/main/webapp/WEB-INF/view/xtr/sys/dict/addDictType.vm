<!DOCTYPE html>
<html>

<head>
    <title>新增字典分类</title>
    #parse("comm/head.vm")
</head>


<div class="ibox" style="padding-top: 0px">


    <div class="ibox-content">
        <form class="form-inline m-t" id="editForm">
            #if($dict)##修改数据
                <input type="text" id="id" name="id" class="hidden" value="$dict.id"/>
                <input type="text" id="parentId" name="parentId" class="hidden" value="$dict.parentId"/>
            #else
                <input type="text" id="parentId" name="parentId" class="hidden" value="$parentId"/>
            #end


            <div class="form-group">
                <label class="col-sm-3 control-label">分类名称：</label>

                <div class="col-sm-8">
                    <input id="dictName" name="dictName" class="form-control" type="text"
                           #if($dict)value="$dict.dictName" #end>
                </div>
            </div>

            <div class="form-group">
                <label class="col-sm-3 control-label">关键字：</label>

                <div class="col-sm-8">
                    <input id="dictValue" name="dictValue" class="form-control" type="text" onpaste="return false"
                           ondragenter="return false" onkeyup="this.value=replaceChinese(this.value)"
                           #if($dict)value="$dict.dictValue" readonly="readonly"#end>
                </div>
            </div>


            <div class="form-group">
                <label class="col-sm-3 control-label">排序：</label>

                <div class="col-sm-8">
                    <input id="sort" name="sort" class="form-control" type="number"
                           #if($dict)value="$dict.sort"#else value="0" #end>
                </div>
            </div>

        </form>
    </div>
</div>
<script src="#springUrl('/resource/js/sys/dict/addDictType.js')"></script>
<script>
    function check(str) {
        var temp = ""
        for (var i = 0; i < str.length; i++)
            if (str.charCodeAt(i) > 0 && str.charCodeAt(i) < 255)
                temp += str.charAt(i)
        return temp
    }
</script>
</html>
